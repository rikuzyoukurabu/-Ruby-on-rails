<h2>
  <%= link_to 'Add New',new_post_path,class: 'header-menu' %>
  my poste
  </h2>
<ul>
  <% @posts.each do |post| %>
  <li>
    <%= link_to post.title,post_path(post) %>
    <%= link_to '[Edit]',edit_post_path(post),class: 'command' %>
    <%= link_to '[destroy]', post_path(post), method: :delete,  class: 'command',data: { confirm: 'Sure?' } %>
  <%= post.title %></li>
  <% end %>
</ul>